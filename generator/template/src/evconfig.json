{
    "routerMode": "<%= ROUTING_MODE %>",
    "mountId": "app",
    "apiURL": "<%= API_URL %>",
    "i18n": {
        "enabled": <%- I18N_ENABLED %>,
        "defaultLocale": "<%= DEFAULT_LOCALE %>",
        "fallbackLocale": "<%= FALLBACK_LOCALE %>",
        "modules": true
    },
    "security": {
        "key": "<%= AES_KEY %>"
    },
    "storage": {
        "name": "everflow",
        "driver": ["asyncStorage"],
        "size": 4980736,
        "storeName": "app"
    },
    "datetime": {
        "date": {
            "format": "<%= DATE_FORMAT %>"
        },
        "time": {
            "format": "<%= TIME_FORMAT %>"
        }
    }
}
